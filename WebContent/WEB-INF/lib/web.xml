<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>catcharide</display-name>

	<servlet>
		<servlet-name>regis</servlet-name>
		<servlet-class>jdbc.Registration</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>regis</servlet-name>
		<url-pattern>/regServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>jdbc.Login</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/loginServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>jdbc.Logout</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logoutServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>createcar</servlet-name>
		<servlet-class>jdbc.CreateCar</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>createcar</servlet-name>
		<url-pattern>/createcarServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>editcar</servlet-name>
		<servlet-class>jdbc.EditCar</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>editcar</servlet-name>
		<url-pattern>/editcarServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>editprofile</servlet-name>
		<servlet-class>jdbc.EditProfile</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>editprofile</servlet-name>
		<url-pattern>/editProfileServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>editcarrent</servlet-name>
		<servlet-class>jdbc.EditCarrent</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>editcarrent</servlet-name>
		<url-pattern>/editcarrentServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>createcarrent</servlet-name>
		<servlet-class>jdbc.CreateCarrent</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>createcarrent</servlet-name>
		<url-pattern>/createcarrentServlet</url-pattern>
	</servlet-mapping>

</web-app>